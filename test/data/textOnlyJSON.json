{
  "elements": [
    {
      "tag": "div",
      "attr": {
        "style": {
          "width": "500px",
          "height": "400px",
          "top": "0",
          "left": "0",
          "position": "fixed",
          "background-color": "#fff"
        }
      }
    },
    {
      "tag": "p",
      "attr": {
        "class": "main-text",
        "style": {
          "line-height": "60px",
          "font-size": "48px",
          "font-weight": "bold",
          "position": "fixed",
          "top": "300px",
          "left": "400px",
          "max-width": "700px",
          "text-align": "center",
          "margin": "0",
          "transform": "translate(-50%, -50%)",
          "text-shadow": "5px 3px 20px rgba(21, 20, 20, 0.39)"
        }
      },
      "className": ["some-class", "other-class"],
      "innerHTML": "${text | 111}",
      "name": "mainText"
    }
  ],
  "width": "${width | 800}",
  "height": "${height | 600}"
}
